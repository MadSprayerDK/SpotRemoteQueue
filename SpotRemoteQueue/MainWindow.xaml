        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label Content="Username:" HorizontalAlignment="Left" Margin="147,80,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="147,106,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="Login_Username"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="147,160,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="Login_Password"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="147,188,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-1.64,0.545" Name="Login_Button" Click="Login_Button_OnClick"/>
